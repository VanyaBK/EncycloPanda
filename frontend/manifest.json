{
  "manifest_version": 2,

  "name": "EncycloPanda",
  "description": "This extension tells people comments about a webpage",
  "version": "1.0",
  "icons": {
    "16": "images/panda.png",
    "48": "images/panda.png",
    "128": "images/panda.png"
  },
  "browser_action": {
    "default_icon": "images/panda.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}
